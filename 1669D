#include<iostream>
int main(){
    int n;
    std::cin>>n;
    bool r(0),b(0);
    for(int i=0; i<n; i++){
        char l=0;
        std::cin>>l;
        if(l=='W'){
            if( r!=b){
                std::cout<< "NO"<< std::endl;
                for(int j=i+1; j<n; j++){
                    std::cin>>l;
                }
            }

        }

    }


}
